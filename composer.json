{
  "name": "aland20/filament-template",
  "type": "project",
  "description": "The Laravel Framework with Filament v3.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1|^8.2",
    "alperenersoy/filament-export": "3.x-dev",
    "bezhansalleh/filament-exceptions": "^2.0.0",
    "bezhansalleh/filament-language-switch": "^2.0.1",
    "doctrine/dbal": "^3.6.5",
    "fakerphp/faker": "^1.23.0",
    "filament/filament": "^3.0-stable",
    "filament/forms": "^3.0-stable",
    "filament/notifications": "^3.0-stable",
    "filament/spatie-laravel-media-library-plugin": "^3.0-stable",
    "filament/tables": "^3.0-stable",
    "guzzlehttp/guzzle": "^7.7.0",
    "laravel/breeze": "^1.23.0",
    "laravel/framework": "^10.18.0",
    "laravel/sanctum": "^3.2.5",
    "laravel/tinker": "^2.8.1",
    "livewire/livewire": "^3.0@beta",
    "savannabits/filament-flatpickr": "^3.0.1",
    "shuvroroy/filament-spatie-laravel-backup": "^2.0.0",
    "spatie/laravel-activitylog": "^4.7.3",
    "spatie/laravel-backup": "^8.3.1",
    "spatie/laravel-ignition": "^2.2.0",
    "spatie/laravel-permission": "^5.10.2",
    "stijnvanouplines/blade-country-flags": "^1.0.3",
    "z3d0x/filament-logger": "^0.6.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.8.2",
    "filament/upgrade": "^3.0-stable",
    "laravel/pint": "^1.10",
    "laravel/sail": "^1.23.2",
    "mockery/mockery": "^1.6.6",
    "nunomaduro/collision": "^7.8.1",
    "pestphp/pest": "^2.13.0",
    "phpunit/phpunit": "^10.3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/globals.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan filament:upgrade"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
