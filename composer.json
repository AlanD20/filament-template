{
  "name": "aland20/filament-template",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1|^8.2",
    "alperenersoy/filament-export": "dev-non-paginated-table-export",
    "bezhansalleh/filament-exceptions": "^1.1.2",
    "bezhansalleh/filament-language-switch": "^1.0.7",
    "doctrine/dbal": "^3.6.1",
    "fakerphp/faker": "^1.22.0",
    "filament/filament": "^2.17.43",
    "filament/forms": "^2.17.43",
    "filament/notifications": "^2.17.43",
    "filament/spatie-laravel-media-library-plugin": "^2.0",
    "filament/tables": "^2.17.43",
    "guzzlehttp/guzzle": "^7.7",
    "laravel/breeze": "^1.19.2",
    "laravel/framework": "^9.52.8",
    "laravel/sanctum": "^3.2.1",
    "laravel/tinker": "^2.8.1",
    "livewire/livewire": "^2.12.3",
    "marjose123/filament-lockscreen": "^1.1.3",
    "marjose123/filament-no-connection": "^1.0.1",
    "saade/filament-laravel-log": "^1.2",
    "savannabits/filament-flatpickr": "^1.1.0",
    "shuvroroy/filament-spatie-laravel-backup": "^1.3",
    "spatie/laravel-activitylog": "^4.7.3",
    "spatie/laravel-backup": "^8.1",
    "spatie/laravel-ignition": "^1.6.4",
    "spatie/laravel-permission": "^5.10.1",
    "stijnvanouplines/blade-country-flags": "^1.0.3",
    "z3d0x/filament-logger": "^0.5.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.8.1",
    "laravel/pint": "^1.10",
    "laravel/sail": "^1.21.2",
    "mockery/mockery": "^1.5.1",
    "nunomaduro/collision": "^6.4",
    "phpunit/phpunit": "^9.6.5",
    "pestphp/pest": "^1.23"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/globals.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan filament:upgrade"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/AlanD20/filament-export"
    }
  ]
}