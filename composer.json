{
  "name": "entensy/filament-template",
  "type": "project",
  "description": "The Laravel Framework with Filament v3.",
  "keywords": [
    "entensy",
    "framework",
    "laravel",
    "filament",
    "livewire"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.1.1",
    "guzzlehttp/guzzle": "^7.8",
    "laravel/breeze": "^2.x-dev",
    "laravel/sanctum": "^4.x-dev",
    "laravel/tinker": "^2.x-dev",
    "livewire/livewire": "^3.4.9",
    "fakerphp/faker": "^1.23.0",
    "filament/filament": "^3.2.61",
    "alperenersoy/filament-export": "3.x-dev",
    "filament/spatie-laravel-media-library-plugin": "^3.2.61",
    "bezhansalleh/filament-language-switch": "^3.0.6",
    "savannabits/filament-flatpickr": "^3.1.0",
    "spatie/laravel-activitylog": "^4.8.0",
    "spatie/laravel-backup": "^8.6.0",
    "spatie/laravel-ignition": "^2.5.0",
    "spatie/laravel-permission": "^6.4.0",
    "z3d0x/filament-logger": "^0.7.0",
    "entensy/filament-tracer": "dev-main",
    "stijnvanouplines/blade-country-flags": "^1.0.5",
    "shuvroroy/filament-spatie-laravel-backup": "^2.1.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.12.2",
    "laravel/pint": "^1.15.0",
    "laravel/sail": "^1.29.1",
    "mockery/mockery": "^1.6.11",
    "nunomaduro/collision": "^8.1.1",
    "pestphp/pest": "^2.34.6",
    "pestphp/pest-plugin-laravel": "^2.3",
    "pestphp/pest-plugin-livewire": "^2.1",
    "phpunit/phpunit": "^10.5.16"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/globals.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan filament:upgrade"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage-html coverage",
    "format": "vendor/bin/pint app config database lang routes tests --config pint.json",
    "push": [
      "php artisan icons:cache",
      "php artisan filament:cache-components"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/entensy/filament-tracer.git",
      "options": {
        "symlink": true
      }
    }
  ]
}
